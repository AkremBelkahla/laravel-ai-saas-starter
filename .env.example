APP_NAME="AI SaaS Starter"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US
APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

# Frontend URL (for CORS, OAuth redirects)
FRONTEND_URL=http://localhost:3000

# Database
DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=laravel_ai_saas
DB_USERNAME=sail
DB_PASSWORD=password

# Session & Cache
SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis

CACHE_STORE=redis
CACHE_PREFIX=

# Redis
REDIS_CLIENT=predis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail
MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# AWS S3 (for image storage)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Stripe
STRIPE_KEY=pk_test_
STRIPE_SECRET=sk_test_
STRIPE_WEBHOOK_SECRET=whsec_
CASHIER_CURRENCY=usd
CASHIER_CURRENCY_LOCALE=en

# Stripe Price IDs (create these in Stripe Dashboard)
STRIPE_PRICE_FREE=
STRIPE_PRICE_PRO=price_
STRIPE_PRICE_TEAM=price_

# OpenAI
OPENAI_API_KEY=sk-
OPENAI_ORGANIZATION=
OPENAI_REQUEST_TIMEOUT=30

# OAuth - Google
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI="${APP_URL}/auth/google/callback"

# OAuth - GitHub
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_REDIRECT_URI="${APP_URL}/auth/github/callback"

# Sanctum
SANCTUM_STATEFUL_DOMAINS=localhost,localhost:3000,127.0.0.1,127.0.0.1:8000,::1

# Horizon
HORIZON_DOMAIN=localhost
HORIZON_PATH=horizon

# Rate Limiting
RATE_LIMIT_FREE=60
RATE_LIMIT_PRO=600
RATE_LIMIT_TEAM=1200

# AI Credits (monthly allocation)
CREDITS_FREE_TEXT=10000
CREDITS_FREE_IMAGE=5
CREDITS_PRO_TEXT=100000
CREDITS_PRO_IMAGE=100
CREDITS_TEAM_TEXT=500000
CREDITS_TEAM_IMAGE=500

# AI Cost per operation (in credits)
COST_TEXT_PER_1K_TOKENS=10
COST_IMAGE_SMALL=100
COST_IMAGE_MEDIUM=200
COST_IMAGE_LARGE=300

# Logging
LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Vite
VITE_APP_NAME="${APP_NAME}"
